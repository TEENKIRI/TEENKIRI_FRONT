{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      qnaList: [],\n      // 초기값은 빈 배열\n      currentPage: 1,\n      totalPages: 1\n    };\n  },\n  async mounted() {\n    await this.fetchQnAList(); // 컴포넌트가 마운트될 때 Q&A 리스트를 불러옵니다.\n  },\n  methods: {\n    async fetchQnAList() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/user/qna`);\n        if (response.status === 200) {\n          this.qnaList = response.data.result; // 응답 데이터에서 Q&A 리스트를 추출\n          this.totalPages = Math.ceil(this.qnaList.length / 10); // 페이지 수 계산\n        } else {\n          console.error('Q&A 리스트 가져오기 실패:', response.data.message || response.statusText);\n        }\n      } catch (error) {\n        console.error('Q&A 리스트 가져오기 중 오류 발생:', error.response ? error.response.data.message : error.message);\n      }\n    },\n    formatDate(dateString) {\n      return new Date(dateString).toLocaleDateString('ko-KR');\n    },\n    viewDetails(id) {\n      this.$router.push(`/qna/detail/${id}`); // ID를 포함한 상세 페이지 URL로 이동\n    },\n    createNewQuestion() {\n      this.$router.push('/qna/create'); // 질문 작성 페이지로 이동\n    },\n    editItem(id) {\n      this.$router.push(`/qna/edit/${id}`); // 수정 페이지로 이동\n    },\n    deleteItem(id) {\n      if (confirm('정말 삭제하시겠습니까?')) {\n        // 삭제 로직 추가\n        console.log(`Item with ID ${id} deleted.`);\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n      this.fetchQnAList(); // 페이지 변경 시 리스트 새로고침\n    }\n  }\n};","map":{"version":3,"names":["axios","data","qnaList","currentPage","totalPages","mounted","fetchQnAList","methods","response","get","process","env","VUE_APP_API_BASE_URL","status","result","Math","ceil","length","console","error","message","statusText","formatDate","dateString","Date","toLocaleDateString","viewDetails","id","$router","push","createNewQuestion","editItem","deleteItem","confirm","log","goToPage","page"],"sources":["/Users/milcho/Project/TEENKIRI/teenkiri_front/teenkiri_front/src/views/user/myQnAListPage.vue"],"sourcesContent":["<template>\n  <div class=\"board-container\">\n    <div class=\"inner\">\n      <h1 class=\"board-title\">내 질문 목록</h1>\n\n      <table class=\"tbl_list\">\n        <caption></caption>\n        <colgroup>\n          <col width=\"80\" />\n          <col width=\"\" />\n          <col width=\"140\" />\n          <col width=\"140\" />\n          <col width=\"140\" />\n        </colgroup>\n        <thead>\n          <tr>\n            <th>번호</th>\n            <th>제목</th>\n            <th>답변일</th>\n            <th>작성일</th>\n            <th>관리</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(item, index) in qnaList\" :key=\"item.id\">\n            <td>{{ index + 1 }}</td>\n            <td class=\"text_left\"><a href=\"javascript:void(0)\" @click=\"viewDetails(item.id)\" class=\"subject\">{{ item.title }}</a></td>\n            <td>{{ item.answeredAt ? formatDate(item.answeredAt) : '미답변' }}</td>\n            <td>{{ formatDate(item.createdTime) }}</td>\n            <td>\n              <button type=\"button\" class=\"btn_adm_control\"></button>\n              <div class=\"conLayer\">\n                <a href=\"javascript:void(0)\" @click=\"editItem(item.id)\" class=\"btn_board_modify\">수정</a>\n                <a href=\"javascript:void(0)\" @click=\"deleteItem(item.id)\" class=\"btn_board_del btn_del\">삭제</a>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n\n      <div class=\"btnWrap\">\n        <a href=\"javascript:void(0)\" @click=\"createNewQuestion\" class=\"btn_write\">작성하기</a>\n      </div>\n\n      <div class=\"pagingWrap\">\n        <ul>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_start\"></a></li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_prev\"></a></li>\n          <li v-for=\"page in totalPages\" :key=\"page\">\n            <a href=\"javascript:void(0)\" @click=\"goToPage(page)\" :class=\"{ btn_paging: true, active: currentPage === page }\">{{ page }}</a>\n          </li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_next\"></a></li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_end\"></a></li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      qnaList: [], // 초기값은 빈 배열\n      currentPage: 1,\n      totalPages: 1,\n    };\n  },\n  async mounted() {\n    await this.fetchQnAList(); // 컴포넌트가 마운트될 때 Q&A 리스트를 불러옵니다.\n  },\n  methods: {\n    async fetchQnAList() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/user/qna`);\n        if (response.status === 200) {\n          this.qnaList = response.data.result; // 응답 데이터에서 Q&A 리스트를 추출\n          this.totalPages = Math.ceil(this.qnaList.length / 10); // 페이지 수 계산\n        } else {\n          console.error('Q&A 리스트 가져오기 실패:', response.data.message || response.statusText);\n        }\n      } catch (error) {\n        console.error('Q&A 리스트 가져오기 중 오류 발생:', error.response ? error.response.data.message : error.message);\n      }\n    },\n    formatDate(dateString) {\n      return new Date(dateString).toLocaleDateString('ko-KR');\n    },\n    viewDetails(id) {\n      this.$router.push(`/qna/detail/${id}`); // ID를 포함한 상세 페이지 URL로 이동\n    },\n    createNewQuestion() {\n      this.$router.push('/qna/create'); // 질문 작성 페이지로 이동\n    },\n    editItem(id) {\n      this.$router.push(`/qna/edit/${id}`); // 수정 페이지로 이동\n    },\n    deleteItem(id) {\n      if (confirm('정말 삭제하시겠습니까?')) {\n        // 삭제 로직 추가\n        console.log(`Item with ID ${id} deleted.`);\n      }\n    },\n    goToPage(page) {\n      this.currentPage = page;\n      this.fetchQnAList(); // 페이지 변경 시 리스트 새로고침\n    }\n  }\n};\n</script>\n\n<style scoped>\n.board-container {\n  width: 90%;\n  margin: 0 auto;\n  padding-top: 50px;\n}\n\n.inner {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 0 20px;\n}\n\n.board-title {\n  font-size: 26px;\n  font-weight: bold;\n  margin-bottom: 20px;\n}\n\n.tbl_list {\n  width: 100%;\n  border-collapse: collapse;\n  margin-bottom: 20px;\n}\n\n.tbl_list th,\n.tbl_list td {\n  border: 1px solid #ccc;\n  padding: 10px;\n  text-align: left;\n}\n\n.tbl_list th {\n  background-color: #f4f4f4;\n}\n\n.text_left {\n  text-align: left;\n}\n\n.subject {\n  cursor: pointer;\n  color: #333;\n  text-decoration: none;\n}\n\n.subject:hover {\n  text-decoration: underline;\n}\n\n.btn_adm_control {\n  background: url('../assets/img/icon_admin_control.png') no-repeat center center;\n  width: 24px;\n  height: 24px;\n  border: none;\n  cursor: pointer;\n}\n\n.conLayer {\n  display: none;\n  position: absolute;\n  background: #fff;\n  border: 1px solid #ccc;\n  z-index: 100;\n}\n\n.btn_adm_control:hover + .conLayer,\n.conLayer:hover {\n  display: block;\n}\n\n.btn_board_modify,\n.btn_board_del {\n  display: block;\n  padding: 6px 12px;\n  background-color: #1976d2;\n  color: #fff;\n  border: none;\n  cursor: pointer;\n  font-size: 14px;\n  text-transform: uppercase;\n  margin-bottom: 5px;\n}\n\n.btn_board_modify:hover {\n  background-color: #1565c0;\n}\n\n.btn_board_del {\n  background-color: #d32f2f;\n}\n\n.btn_board_del:hover {\n  background-color: #b71c1c;\n}\n\n.btnWrap {\n  text-align: right;\n  margin-top: 20px;\n}\n\n.btn_write {\n  padding: 12px 25px;\n  background-color: #333;\n  color: #fff;\n  border: none;\n  cursor: pointer;\n  font-size: 14px;\n  text-transform: uppercase;\n}\n\n.btn_write:hover {\n  background-color: #555;\n}\n\n.pagingWrap ul {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n  text-align: center;\n  margin-top: 20px;\n}\n\n.pagingWrap li {\n  display: inline-block;\n}\n\n.pagingWrap li a {\n  margin: 0 5px;\n  text-decoration: none;\n  color: black;\n  cursor: pointer;\n}\n\n.pagingWrap li a.active {\n  font-weight: bold;\n  color: blue;\n}\n\n.pagingWrap .btn_paging_start:before {\n  content: \"<<\";\n}\n\n.pagingWrap .btn_paging_prev:before {\n  content: \"<\";\n}\n\n.pagingWrap .btn_paging_next:before {\n  content: \">\";\n}\n\n.pagingWrap .btn_paging_end:before {\n  content: \">>\";\n}\n</style>\n"],"mappings":";AA4DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MAAE;MACbC,WAAW,EAAE,CAAC;MACdC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,YAAY,CAAC,CAAC,EAAE;EAC7B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,GAAGC,OAAO,CAACC,GAAG,CAACC,oBAAoB,WAAW,CAAC;QAChF,IAAIJ,QAAQ,CAACK,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACX,OAAM,GAAIM,QAAQ,CAACP,IAAI,CAACa,MAAM,EAAE;UACrC,IAAI,CAACV,UAAS,GAAIW,IAAI,CAACC,IAAI,CAAC,IAAI,CAACd,OAAO,CAACe,MAAK,GAAI,EAAE,CAAC,EAAE;QACzD,OAAO;UACLC,OAAO,CAACC,KAAK,CAAC,kBAAkB,EAAEX,QAAQ,CAACP,IAAI,CAACmB,OAAM,IAAKZ,QAAQ,CAACa,UAAU,CAAC;QACjF;MACF,EAAE,OAAOF,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACX,QAAO,GAAIW,KAAK,CAACX,QAAQ,CAACP,IAAI,CAACmB,OAAM,GAAID,KAAK,CAACC,OAAO,CAAC;MACtG;IACF,CAAC;IACDE,UAAUA,CAACC,UAAU,EAAE;MACrB,OAAO,IAAIC,IAAI,CAACD,UAAU,CAAC,CAACE,kBAAkB,CAAC,OAAO,CAAC;IACzD,CAAC;IACDC,WAAWA,CAACC,EAAE,EAAE;MACd,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,eAAeF,EAAE,EAAE,CAAC,EAAE;IAC1C,CAAC;IACDG,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,aAAa,CAAC,EAAE;IACpC,CAAC;IACDE,QAAQA,CAACJ,EAAE,EAAE;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,aAAaF,EAAE,EAAE,CAAC,EAAE;IACxC,CAAC;IACDK,UAAUA,CAACL,EAAE,EAAE;MACb,IAAIM,OAAO,CAAC,cAAc,CAAC,EAAE;QAC3B;QACAf,OAAO,CAACgB,GAAG,CAAC,gBAAgBP,EAAE,WAAW,CAAC;MAC5C;IACF,CAAC;IACDQ,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACjC,WAAU,GAAIiC,IAAI;MACvB,IAAI,CAAC9B,YAAY,CAAC,CAAC,EAAE;IACvB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}