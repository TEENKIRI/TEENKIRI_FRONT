{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      questions: [],\n      loading: false,\n      headers: [{\n        text: 'ID',\n        value: 'id'\n      }, {\n        text: '작성자',\n        value: 'questionUserName'\n      }, {\n        text: '제목',\n        value: 'title'\n      }, {\n        text: '생성 시간',\n        value: 'createdTime'\n      }, {\n        text: '수정 시간',\n        value: 'updatedTime'\n      }, {\n        text: '상세보기',\n        value: 'actions',\n        sortable: false\n      }],\n      options: {\n        itemsPerPage: 10\n      },\n      currentPage: 1,\n      totalPages: 1\n    };\n  },\n  created() {\n    this.fetchQuestions();\n  },\n  methods: {\n    async fetchQuestions() {\n      this.loading = true;\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/qna/list`, {\n          params: {\n            page: this.currentPage - 1,\n            size: this.options.itemsPerPage\n          }\n        });\n\n        // 실제 응답 데이터를 확인해 보세요.\n        console.log('API Response:', response.data);\n\n        // 응답 데이터 구조에 맞게 접근하도록 수정\n        const data = response.data.data; // CommonResDto의 'data' 필드 접근\n        if (data && data.content) {\n          this.questions = data.content;\n          this.totalPages = data.totalPages;\n        } else {\n          console.error('올바르지 않은 데이터 형식입니다:', response.data);\n        }\n      } catch (error) {\n        console.error('질문 목록을 불러오는 중 오류가 발생했습니다:', error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    formatDate(date) {\n      const options = {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      };\n      return new Date(date).toLocaleDateString('ko-KR', options);\n    },\n    createNewQuestion() {\n      this.$router.push('/qna/create');\n    },\n    viewDetail(id) {\n