{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"QnAList\",\n  data() {\n    return {\n      qnaList: [],\n      currentPage: 1,\n      totalPages: 1\n    };\n  },\n  async mounted() {\n    await this.fetchQnAList();\n  },\n  methods: {\n    async fetchQnAList() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/user/qna`);\n        if (response.status === 200) {\n          this.qnaList = response.data.result;\n          this.totalPages = Math.ceil(this.qnaList.length / 10); // assuming 10 items per page\n        } else {\n          console.error('Q&A 리스트 가져오기 실패:', response.data.message || response.statusText);\n        }\n      } catch (error) {\n        console.error('Q&A 리스트 가져오기 중 오류 발생:', error.response ? error.response.data.message : error.message);\n      }\n    },\n    formatDate(dateString) {\n      const d = new Date(dateString);\n      const year = d.getFullYear();\n      const month = String(d.getMonth() + 1).padStart(2, '0');\n      const day = String(d.getDate()).padStart(2, '0');\n      return `${year}-${month}-${day}`;\n    },\n    viewDetails(id) {\n      this.$router.push(`/qna/detail/${id}`);\n    },\n    deleteItem(id) {\n      // delete logic\n    },\n    createNewQuestion() {\n      this.$router.push('/qna/create');\n    },\n    goToPage(page) {\n      this.currentPage = page;\n      this.fetchQnAList(); // refetch or handle pagination\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","qnaList","currentPage","totalPages","mounted","fetchQnAList","methods","response","get","process","env","VUE_APP_API_BASE_URL","status","result","Math","ceil","length","console","error","message","statusText","formatDate","dateString","d","Date","year","getFullYear","month","String","getMonth","padStart","day","getDate","viewDetails","id","$router","push","deleteItem","createNewQuestion","goToPage","page"],"sources":["/Users/milcho/Project/TEENKIRI/teenkiri_front/teenkiri_front/src/views/user/myQnAListPage.vue"],"sourcesContent":["<template>\n  <div class=\"boardWrap\">\n    <div class=\"inner\">\n      <h1 class=\"board-title\">내 질문 목록</h1>\n\n      <table class=\"tbl_list\">\n        <caption></caption>\n        <colgroup>\n          <col width=\"80\" />\n          <col width=\"\" />\n          <col width=\"140\" />\n          <col width=\"140\" />\n          <col width=\"160\" />\n        </colgroup>\n        <thead>\n          <tr>\n            <th>번호</th>\n            <th>제목</th>\n            <th>답변일</th>\n            <th>작성일</th>\n            <th>보기</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(item, index) in qnaList\" :key=\"item.id\">\n            <td>{{ index + 1 }}</td>\n            <td class=\"text_left\"><a href=\"javascript:void(0)\" @click=\"viewDetails(item.id)\" class=\"subject\">{{ item.title }}</a></td>\n            <td>{{ item.answeredAt ? formatDate(item.answeredAt) : '미답변' }}</td>\n            <td>{{ formatDate(item.createdTime) }}</td>\n            <td>\n              <button type=\"button\" @click=\"viewDetails(item.id)\" class=\"btn_adm_control\"></button>\n              <div class=\"conLayer\">\n                <a href=\"javascript:void(0)\" @click=\"viewDetails(item.id)\" class=\"btn_board_modify\">수정</a>\n                <a href=\"javascript:void(0)\" @click=\"deleteItem(item.id)\" class=\"btn_board_del btn_del\">삭제</a>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n\n      <div class=\"btnWrap\">\n        <button type=\"button\" @click=\"createNewQuestion\" class=\"btn_write\">작성하기</button>\n      </div>\n      \n      <div class=\"pagingWrap\">\n        <ul>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_start\"></a></li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_prev\"></a></li>\n          <li v-for=\"page in totalPages\" :key=\"page\">\n            <a href=\"javascript:void(0)\" @click=\"goToPage(page)\" :class=\"['btn_paging', { active: currentPage === page }]\">{{ page }}</a>\n          </li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_next\"></a></li>\n          <li><a href=\"javascript:void(0)\" class=\"btn_paging_end\"></a></li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: \"QnAList\",\n  data() {\n    return {\n      qnaList: [],\n      currentPage: 1,\n      totalPages: 1,\n    };\n  },\n  async mounted() {\n    await this.fetchQnAList();\n  },\n  methods: {\n    async fetchQnAList() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/user/qna`);\n        if (response.status === 200) {\n          this.qnaList = response.data.result;\n          this.totalPages = Math.ceil(this.qnaList.length / 10); // assuming 10 items per page\n        } else {\n          console.error('Q&A 리스트 가져오기 실패:', response.data.message || response.statusText);\n        }\n      } catch (error) {\n        console.error('Q&A 리스트 가져오기 중 오류 발생:', error.response ? error.response.data.message : error.message);\n      }\n    },\n    formatDate(dateString) {\n      const d = new Date(dateString);\n      const year = d.getFullYear();\n      const month = String(d.getMonth() + 1).padStart(2, '0');\n      const day = String(d.getDate()).padStart(2, '0');\n      return `${year}-${month}-${day}`;\n    },\n    viewDetails(id) {\n      this.$router.push(`/qna/detail/${id}`);\n    },\n    deleteItem(id) {\n      // delete logic\n    },\n    createNewQuestion() {\n      this.$router.push('/qna/create');\n    },\n    goToPage(page) {\n      this.currentPage = page;\n      this.fetchQnAList(); // refetch or handle pagination\n    }\n  }\n};\n</script>\n\n<style scoped>\n.boardWrap {\n  width: 90%;\n  margin: 0 auto;\n  padding-top: 50px;\n}\n\n.inner {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 0 20px;\n}\n\n.board-title {\n  font-size: 26px;\n  font-weight: bold;\n  margin-bottom: 20px;\n}\n\n.tbl_list {\n  width: 100%;\n  border-collapse: collapse;\n  margin-bottom: 20px;\n}\n\n.tbl_list th,\n.tbl_list td {\n  border: 1px solid #ccc;\n  padding: 10px;\n  text-align: left;\n}\n\n.tbl_list th {\n  background-color: #f4f4f4;\n}\n\n.text_left {\n  text-align: left;\n}\n\n.subject {\n  cursor: pointer;\n  color: #333;\n  text-decoration: none;\n}\n\n.subject:hover {\n  text-decoration: underline;\n}\n\n.btn_adm_control {\n  background-image: url('../assets/img/icon_admin_control.png');\n  width: 24px;\n  height: 24px;\n  background-size: cover;\n  border: none;\n  cursor: pointer;\n}\n\n.conLayer {\n  display: none;\n}\n\n.btn_adm_control:focus + .conLayer,\n.btn_adm_control:hover + .conLayer {\n  display: block;\n}\n\n.btn_board_modify,\n.btn_board_del {\n  display: block;\n  margin: 5px 0;\n  color: #333;\n  text-decoration: none;\n}\n\n.btn_board_del {\n  color: #d32f2f;\n}\n\n.btnWrap {\n  text-align: right;\n  margin-top: 20px;\n}\n\n.btn_write {\n  padding: 12px 25px;\n  background-color: #333;\n  color: #fff;\n  border: none;\n  cursor: pointer;\n}\n\n.btn_write:hover {\n  background-color: #555;\n}\n\n.pagingWrap ul {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n  text-align: center;\n  margin-top: 20px;\n}\n\n.pagingWrap li {\n  display: inline-block;\n}\n\n.pagingWrap li a {\n  margin: 0 5px;\n  text-decoration: none;\n  color: black;\n  cursor: pointer;\n}\n\n.pagingWrap li a.active {\n  font-weight: bold;\n  color: blue;\n}\n\n.pagingWrap .btn_paging_start:before {\n  content: \"<<\";\n}\n\n.pagingWrap .btn_paging_prev:before {\n  content: \"<\";\n}\n\n.pagingWrap .btn_paging_next:before {\n  content: \">\";\n}\n\n.pagingWrap .btn_paging_end:before {\n  content: \">>\";\n}\n</style>\n"],"mappings":";AA4DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE,CA