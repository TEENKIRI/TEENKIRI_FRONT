>\n    <!-- </v-sheet> -->\n\n    <!-- 과목 선택 섹션 -->\n    <v-btn\n      class=\"mb-2\"\n      color=\"success\"\n      @click=\"$router.push('/course/create')\"\n      v-if=\"this.user.role === `ADMIN`\"\n      >과목 업로드</v-btn\n    >\n    <v-row>\n      <v-col cols=\"12\" class=\"py-2\">\n        <v-btn-toggle\n          v-model=\"course.selectedMenu\"\n          color=\"primary\"\n          rounded=\"0\"\n          group\n          class=\"d-flex flex-row\"\n          @change=\"performSearch\"\n        >\n          <v-btn class=\"flex-grow-1\" value=\"all\"> all </v-btn>\n          <v-btn\n            class=\"flex-grow-1\"\n            v-for=\"c in course.courseList\"\n            :key=\"c.id\"\n            :value=\"c.id\"\n          >\n            {{ c.title }}\n            <span\n              v-if=\"this.user.role === `ADMIN`\"\n              class=\"mdi mdi-pencil\"\n              style=\"\n                width: 24px;\n                height: 24px;\n                font-size: 24px;\n                line-height: 24px;\n                color: #f27885;\n              \"\n              @click=\"editCourse(c.id, $event)\"\n            ></span>\n          </v-btn>\n        </v-btn-toggle>\n      </v-col>\n    </v-row>\n\n    <!-- 학년 선택 및 정렬 -->\n    <v-row>\n      <v-col>\n        <v-item-group\n          selected-class=\"bg-purple\"\n          multiple\n          class=\"d-flex justify-start\"\n          v-model=\"grade.selectedGrades\"\n          @change=\"performSearch\"\n        >\n          <div class=\"mr-5\">학년</div>\n          <v-item\n            v-for=\"n in grade.gradeList\"\n            :key=\"n.value\"\n            v-slot=\"{ selectedClass, toggle }\"\n            :value=\"n.value\"\n          >\n            <v-chip class=\"mr-2\" :class=\"selectedClass\" @click=\"toggle\" :value=\"n.value\">\n              {{ n.text }}\n            </v-chip>\n          </v-item>\n        </v-item-group>\n        <br>\n        <v-row>\n          <v-col class=\"d-flex flex