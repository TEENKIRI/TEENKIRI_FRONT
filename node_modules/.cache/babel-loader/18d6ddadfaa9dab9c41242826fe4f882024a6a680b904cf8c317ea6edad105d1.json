       <div class=\"thumb\">\n                <a href=\"javascript:void(0)\">\n                  <img :src=\"s.subjectThumUrl\" alt=\"Í∞ïÏ¢å Ïç∏ÎÑ§Ïùº\" />\n                </a>\n              </div>\n              <div class=\"txt\">\n                <p class=\"subject\">{{ s.title }}</p>\n                <p class=\"name\">{{ s.teacherName }}</p>\n              </div>\n              <button\n                type=\"button\"\n                class=\"btn_like\"\n                @click=\"toggleWish(s.id, $event)\"\n                :class=\"{\n                  'mdi mdi-heart': s.isSubscribe,\n                  'mdi mdi-heart-outline': !s.isSubscribe,\n                }\"\n              ></button>\n            </div>\n          </div>\n        </v-card-text>\n      </v-card>\n    </v-row>\n\n    <!-- ÌéòÏù¥ÏßÄÎÑ§Ïù¥ÏÖò -->\n    <v-pagination\n      v-model=\"subject.page.currentPage\"\n      :length=\"totalPages\"\n      @input=\"onPageChange\"\n      class=\"my-4\"\n    ></v-pagination>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      user: {},\n      subject: {\n        subjectIsMainList: [],\n        page: {\n          pageSize: 1,  // ÌïúÎ≤àÏóê Î≥¥Ïó¨Ï§Ñ Í∞ïÏ¢å Ïàò (ÏõêÌïòÎäîÎåÄÎ°ú Ï°∞Ï†ï Í∞ÄÎä•)\n          currentPage: 1, \n        },\n      },\n      totalPages: 1,\n    };\n  },\n  async created() {\n    try {\n      await this.$store.dispatch('setUserAllInfoActions');\n      this.user = this.$store.getters.getUserObj;\n    } catch (error) {\n      console.error('ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥Î•º Í∞ÄÏ†∏Ïò§Îäî Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§:', error);\n    }\n    this.getSubjectMainList();\n  },\n  methods: {\n    // ÌéòÏù¥ÏßÄÍ∞Ä Î≥ÄÍ≤ΩÎê† ÎïåÎßàÎã§ Ìò∏Ï∂úÎêòÎäî Î©îÏÑúÎìú\n    onPageChange(page) {\n      this.subject.page.currentPage = page;\n      this.getSubjectMainList();\n    },\n\n    // Í∞ïÏ¢å Î™©Î°ùÏùÑ Î∂àÎü¨Ïò§Îäî Î©îÏÑúÎìú (ÌéòÏù¥ÏßÄÎÑ§Ïù¥ÏÖò Ï†ÅÏö©)\n    async getSubjectMainList() {\n      try {\n        const params = {\n          size: this.subject.page.pageSize,  // ÌïúÎ≤àÏóê Î≥¥Ïó¨Ï§Ñ Í∞ïÏ¢å Ïàò\n          page: this.subject.page.currentPage - 1, // APIÏóêÏÑú 0Î∂ÄÌÑ∞ ÏãúÏûëÌïòÎØÄÎ°ú -1 Ìï¥Ï§å\n        };\n        \n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/subject/main/list`, { params });\n        this.subject.subjectIsMainList = response.data.result.content;\n        this.totalPages = response.data.result.totalPages;  // Ï†ÑÏ≤¥ ÌéòÏù¥ÏßÄ Ïàò ÏÑ§Ï†ï\n      } catch (e) {\n        console.error(e);\n      }\n    },\n\n    // Í∞ïÏ¢å ÏÉÅÏÑ∏ ÌéòÏù¥ÏßÄÎ°ú Ïù¥Îèô\n    goToDetail(id) {\n      this.$router.push({ name: 'SubjectDetail', params: { id } });\n    },\n\n    // Ï∞ú ÌÜ†Í∏Ä Í∏∞Îä•\n    async toggleWish(id, event) {\n      event.stopPropagation();  // Ïù¥Î≤§Ìä∏ Ï†ÑÌåå Î∞©ÏßÄ\n      if (Object.keys(this.user).length > 0) { // Î°úÍ∑∏Ïù∏Ìïú Ïú†Ï†ÄÎßå Í∞ÄÎä•\n        try {\n          const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/wish/toggle/${id}`);\n          const subject = this.subject.subjectIsMainList.find(sm => sm.id === id);\n          if (subject) {\n            subject.isSubscribe = response.data.result.status;\n          }\n        } catch (error) {\n          alert('Ï∞ú Ï∂îÍ∞Ä Ïã§Ìå®');\n          console.error(error);\n        }\n      } else {\n        alert('Î°úÍ∑∏Ïù∏ ÌõÑ ÏÇ¨Ïö© Í∞ÄÎä•Ìï©ÎãàÎã§.');\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.select-small .v-select {\n  min-height: 32px;\n  max-height: 32px;\n  font-size: 14px;\n  line-height: 32px;\n}\n\n.v-btn + .v-btn {\n  margin-left: 16px;\n}\n</style>\n"],"mappings":";AA8DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,OAAO,EAAE;QACPC,iBAAiB,EAAE,EAAE;QACrBC,IAAI,EAAE;UACJC,QAAQ,EAAE,CAAC;UAAG;UACdC,WAAW,EAAE;QACf;MACF,CAAC;MACDC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,uBAAuB,CAAC;MACnD,IAAI,CAACT,IAAG,G